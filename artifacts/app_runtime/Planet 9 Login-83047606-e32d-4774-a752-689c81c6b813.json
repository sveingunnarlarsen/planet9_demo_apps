													"fieldName": "formLoginButton",
													"fieldParent": "1dcd49b1-725c-4d75-917d-f8653f6a5efc",
													"fieldType": "sap.m.Button",
													"script": "",
													"request": [],
													"response": [],
													"attributes": [
														{
															"attribute": "width",
															"grouping": "Properties",
															"value": "110px",
															"script": "",
															"translation": []
														},
														{
															"attribute": "type",
															"grouping": "Properties",
															"value": "Emphasized",
															"script": "",
															"translation": []
														},
														{
															"attribute": "text",
															"grouping": "Properties",
															"value": "Log On",
															"script": "",
															"translation": []
														},
														{
															"attribute": "press",
															"grouping": "Events",
															"value": "Anonymous Function",
															"script": [
																"==OBJECT STRING==",
																"var options = {};",
																"options.username = inFormLoginUsername.getValue().toLowerCase();",
																"options.password = inFormLoginPassword.getValue();",
																"options.logonid = inFormLoginTypes.getSelectedKey()",
																"",
																"if (!options.username || !options.password) {",
																"",
																"    if (!options.username) {",
																"        inFormLoginUsername.setValueState(\"Error\");",
																"    } else {",
																"        inFormLoginUsername.setValueState();",
																"    }",
																"",
																"    if (!options.password) {",
																"        inFormLoginPassword.setValueState(\"Error\");",
																"    } else {",
																"        inFormLoginPassword.setValueState();",
																"    }",
																"",
																"    return;",
																"}",
																"",
																"// Handle Logon ",
																"if (isMobile) {",
																"    AppCache.LogonCustom(options);",
																"    inFormLoginUsername.setValue();",
																"    inFormLoginPassword.setValue();",
																"} else {",
																"    if (options.logonid === \"local\") {",
																"        logonLocal(options);",
																"    } else {",
																"        logonLDAP(options);",
																"    }",
																"}"
															],
															"translation": []
														}
													],
