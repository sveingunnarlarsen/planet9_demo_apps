					"data": [
						{
							"fieldNo": "7",
							"fieldName": "init",
							"fieldParent": "36d9be9f-e396-4b0d-bf94-fcdd46951aa9",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"var smtpVerified = false;",
								"var isMobile = false;",
								"var text1 = \"\";",
								"var text2 = \"\";",
								"var text3 = \"\";",
								"var urlPrefix = \"\";",
								"",
								"if (sap && sap.n && sap.n.Shell) {",
								"    console.log(\"AttachInit: \", sap.n.Shell);",
								"}",
								"",
								"var sapCore = sap.ui.getCore();",
								"",
								"sapCore.attachInit(function(data) {",
								"    ",
								"    console.log(\"Inside init\");",
								"",
								"    // On Mobile - Set URL Path prefix",
								"    if (typeof AppCache !== \"undefined\" && AppCache.isMobile) {",
								"        urlPrefix = AppCache.Url;",
								"        isMobile = true;",
								"    }",
								"",
								"    // Fetch Logon Types",
								"    getLogonTypes();",
								"",
								"});"
							],
							"request": [],
							"response": [],
							"attributes": [],
							"data": []
						},
						{
							"fieldNo": "1fd47256-b43d-49ad-9ad6-8bfe6d9ac75b",
							"fieldName": "LogonTypes",
							"fieldParent": "36d9be9f-e396-4b0d-bf94-fcdd46951aa9",
							"fieldType": "neptune.Script",
							"script": [
								"==OBJECT STRING==",
								"function getLogonTypes() {",
								"",
								"    var query = \"\";",
								"",
								"    if (location.pathname.toLowerCase().indexOf(\"/launchpad/\") > -1) {",
								"        var path = location.pathname.split(\"/\");",
								"        query = \"?launchpad=\" + path[path.length - 1];",
								"    }",
								"",
								"    $.ajax({",
								"        type: \"GET\",",
								"        url: urlPrefix + \"/user/logon/types\" + query,",
								"        success: function(data) {",
								"",
								"            // Save Data ",
								"            modelDataCustSettings.setData(data);",
								"            setCacheDataCustSettings();",
								"",
								"            // Handle Startup Actions ",
								"            setSettings();",
								"",
								"        },",
								"        error: function(result, status) {",
								"            setSettings();",
								"        }",
								"    });",
								"}",
								"",
								"function setSettings() {",
								"",
								"    if (!modelDataCustSettings.oData.settings) return;",
								"",
								"    smtpVerified = modelDataCustSettings.oData.showForgotPassword;",
								"",
								"    // If SMTP is verified -> Show forgotpassword",
								"    if (smtpVerified) {",
								"        linkForgot.setVisible(true);",
								"    } else {",
								"        linkForgot.setVisible(false);",
								"    }",
								"",
								"    // Get System Name/Description ",
								"    if (modelDataCustSettings.oData.settings.name) {",
								"        formLogonTextsSubTitle1.setText(modelDataCustSettings.oData.settings.name);",
								"    }",
								"",
								"    if (modelDataCustSettings.oData.settings.description) {",
								"        formLogonTextsSubTitle2.setText(modelDataCustSettings.oData.settings.description);",
								"    }",
								"",
								"    // Parse Logon Types",
								"    var idps = [];",
								"",
								"    inFormLoginTypes.removeAllItems();",
								"    inFormLoginTypes.addItem(new sap.ui.core.Item({",
								"        key: \"local\",",
								"        text: \"Local\"",
								"    }));",
								"",
								"    for (var i = 0, length = modelDataCustSettings.oData.logonTypes.length; i < length; i++) {",
								"        switch (modelDataCustSettings.oData.logonTypes[i].type) {",
								"            case 'ldap':",
								"                inFormLoginTypes.addItem(new sap.ui.core.Item({",
								"                    key: modelDataCustSettings.oData.logonTypes[i].id,",
