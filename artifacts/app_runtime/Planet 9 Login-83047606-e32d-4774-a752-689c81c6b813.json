								"function getLogonTypeInfo(id) {",
								"    var info = {};",
								"    $.each(modelDataCustSettings.oData.logonTypes, function(i, data) {",
								"        if (data.id === id) info = data;",
								"    });",
								"    return info;",
								"}",
								"",
								"function logonLocal(options) {",
								"    $.ajax({",
								"        type: \"POST\",",
								"        contentType: \"application/json\",",
								"        url: \"/user/logon/local\",",
								"        data: JSON.stringify(options),",
								"        success: function(data) {",
								"            location.reload(true);",
								"        },",
								"        error: function(result, status) {",
								"            handleWrongUserPass(result);",
								"        }",
								"    });",
								"}",
								"",
								"function logonLDAP(options) {",
								"",
								"    var logonData = getLogonTypeInfo(options.logonid);",
								"",
								"    $.ajax({",
								"        type: \"POST\",",
								"        contentType: \"application/json\",",
								"        url: \"/user/logon/ldap/\" + logonData.path,",
								"        data: JSON.stringify(options),",
								"        success: function(data) {",
								"            location.reload(true);",
								"        },",
								"        error: function(result, status) {",
								"            handleWrongUserPass(result);",
								"        }",
								"    });",
								"}",
								"",
								"function handleWrongUserPass(result) {",
								"    if (result.status === 401) {",
								"        inFormLoginUsername.setValueState(\"Error\");",
								"        inFormLoginPassword.setValueState(\"Error\");",
								"        sap.m.MessageToast.show(txtWrongUsernamePassword.getText());",
								"    } else {",
								"        if (result.responseJSON) {",
								"            sap.m.MessageToast.show(result.status + \": \" + result.responseJSON.status);",
								"        } else {",
								"            sap.m.MessageToast.show(result.status + \": \" + result.statusText);",
								"        }",
								"    }",
								"}"
